public class sample3{
    public static void main(String[] args) {
        int[] [] a={{2,3,4,5},
                    {1,2,3,4},
                    {8,9,6,5},
                    {3,5,4,8}
                    };
                    for(int i=0;i<a.length;i++) {
                        for(int j=0;j<a.length;j++) {
                            System.out.println(a[i][j]+"\t");
                    }
                    }
    }
}

    
