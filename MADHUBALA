package com.java;
public class FizzBuzz{
    public static void main(String[]args){
        int n=20;
        for(int i=1;i<=n;i++){
            if(i%15==0){
                System.out.println("fizzBuzz");
            }else if(i%3==0){
                System.out.println("fizz");
            }
            else if(i%5==0){
                System.out.println("buzz");
            }else{
            System.out.println(i);
        }
    }
}
}
            
        
