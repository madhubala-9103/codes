public class maxelement{
    public static void main(String[] args){
        int[][]matrix = {
            {10,25,3},
            {4,25,6},
            {17,8,8}
        };
        int max=50;
        for(int[]row:matrix){
            for(int num:row){
            if(num>max);
            max=num;
        }}
        System.out.println("Largest element:"+ max);
            
        }
        }
        
    
